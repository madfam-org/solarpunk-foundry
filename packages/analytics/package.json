{
  "name": "@madfam/analytics",
  "version": "0.1.0",
  "description": "MADFAM shared analytics instrumentation for PostHog",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./next": {
      "import": "./dist/next.mjs",
      "require": "./dist/next.js",
      "types": "./dist/next.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "posthog-js": "^1.96.0"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "analytics",
    "posthog",
    "madfam",
    "tracking"
  ],
  "author": "MADFAM",
  "license": "MIT"
}
