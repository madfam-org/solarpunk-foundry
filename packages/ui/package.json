{
  "name": "@madfam/ui",
  "version": "0.1.0",
  "description": "MADFAM shared UI components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./PricingCalculator": {
      "import": "./dist/components/PricingCalculator.mjs",
      "require": "./dist/components/PricingCalculator.js",
      "types": "./dist/components/PricingCalculator.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "ui",
    "components",
    "madfam",
    "react"
  ],
  "author": "MADFAM",
  "license": "MIT"
}
